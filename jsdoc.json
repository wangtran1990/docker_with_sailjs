{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": ["api/services"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "destination": "docs/services",
    "recurse": true,
    "readme": "README.md"
  }
}
